<config>
    <modules>
        <Itwin_PostcodeComplete>
            <version>0.1.1</version>
        </Itwin_PostcodeComplete>
    </modules>
    <global>
        <helpers>
            <postcodecomplete>
                <class>Itwin_PostcodeComplete_Helper</class>
            </postcodecomplete>
        </helpers>        
        <models>
            <postcodecomplete>
                <class>Itwin_PostcodeComplete_Model</class>
            </postcodecomplete>
        </models>
    </global>
    <frontend>
        <routers>
            <Itwin_PostcodeComplete>
                <use>standard</use>
                <args>
                    <module>Itwin_PostcodeComplete</module>
                    <frontName>postcodecomplete</frontName>
                </args>
            </Itwin_PostcodeComplete>
        </routers>
        <layout>
            <updates>
                <postcodecomplete>
                    <file>itwin_postcodecomplete.xml</file>
                </postcodecomplete>
            </updates>
        </layout>
    </frontend>
    <default>
        <itwin>
            <postcodecomplete>
                <active>1</active>
                <urlwebservice>http://m.correios.com.br/movel/buscaCepConfirma.do</urlwebservice>
            </postcodecomplete>
        </itwin>
    </default>
</config>
